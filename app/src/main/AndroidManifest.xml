<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.ac.um.produkskripsi">

    <!-- <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" /> -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashScreen"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CitraBitmap"
            android:label="@string/title_activity_citra_bitmap"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".materi.KIKD"
            android:label="@string/title_activity_kikd"
            android:screenOrientation="portrait"
            android:parentActivityName=".CitraBitmap"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="id.ac.um.produkskripsi.CitraBitmap" />
        </activity>
        <activity android:name=".materi.ProsesGabung"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".listdata.VideoYoutube"
            android:screenOrientation="landscape" />
        <activity android:name=".Referensi"
            android:screenOrientation="portrait"
            />
        <activity android:name=".kuis.Quiz"
            android:screenOrientation="portrait"
            />
        <activity android:name=".kuis.HighestScore"
            android:screenOrientation="portrait"
            />
        <activity android:name=".materi.submateri.DetailMateri"
            android:screenOrientation="portrait"
            ></activity>
    </application>

</manifest>