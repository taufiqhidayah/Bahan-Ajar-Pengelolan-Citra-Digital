<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.ac.um.produkskripsi">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashScreen"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".CitraBitmap"
            android:label="@string/title_activity_citra_bitmap"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".materi.KIKD"
            android:label="@string/title_activity_kikd"
            android:parentActivityName=".CitraBitmap"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="id.ac.um.produkskripsi.CitraBitmap" />
        </activity>
        <activity android:name=".materi.ProsesGabung"></activity>
    </application>

</manifest>