<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="17dp"
    tools:context="id.ac.um.produkskripsi.materi.evaluasi.ShowEval">

    <Button
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginBottom="15dp"
        android:padding="5dp"
        android:id="@+id/Uji"
        android:layout_centerInParent="true"
        android:background="@color/colorAccent"
        android:text="Uji Kemampuan Pikiran" />

    <Button
        android:id="@+id/Eval"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_above="@+id/Uji"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="25dp"
        android:background="@color/deep_orange_500"
        android:padding="5dp"
        android:text="Uji Kemampuan Pengetahuan" />


</RelativeLayout>
